<script>
(function($context, $doc){
	if (!$context.length) return;
	
	// 스크롤 메뉴
	var $menu = $context.find('.event-menu');
	var $menus = $context.find('.event-menu li');
	var $menubox = $menu.find('.event-menu-inner');
	var $sections = $context.find('section');
	var $fixedBox = $context.find('.fixed-box');
	var offset = 84;

	// 우측 애니메이션 고정
	var setAniFix = function(){
		var windowHeight, $aniBox, aniTop, aniHeight;

		windowHeight = $(window).innerHeight();
		$aniBox = $('.ani-sec');
		aniTop = $('.evt-top').offset().top;
		aniHeight = $('.evt-top').outerHeight();
		sumHeight = aniTop + aniHeight;

		$(window).scrollTop() > sumHeight - windowHeight ? $aniBox.addClass('fixed') : $aniBox.removeClass('fixed');
	}

	// 탭 하단 고정
	var setMenufix2 = function(){
		var windowHeight, topTextTop, topTextHeight, tabHeight, sumHeight;
		
		windowHeight = $(window).innerHeight();
		topTextTop = $('.txt-sub').offset().top;
		topTextHeight = $('.txt-sub').outerHeight();
		tabHeight = $menubox.outerHeight();
		sumHeight = topTextTop + topTextHeight + tabHeight;

		$(window).scrollTop() > sumHeight - windowHeight == true ? $menubox.addClass('fixed') : $menubox.removeClass('fixed');
	}

	// 스크롤 시 탭 on, off
	var setSection = function(){
		$sections.each(function(idx, $section){
			if ($(window).scrollTop() >= $section.offsetTop - offset - 41) {
				$menus.removeClass('active');
				$menus.eq(idx).addClass('active');
			}
		});
	};

	// 탭 클릭 이동
	$menu.on('click', 'li a', function (e) {
		var section = $(e.currentTarget.hash);
		var offsetTop = Math.round(section.offset().top);
		$doc.stop().animate({
			scrollTop: offsetTop - offset,
		}, 400);
		e.preventDefault();
	});

	// 섹션 타이틀 고정
	function setTitlefix() {
		const checkTiming = 400;
		const arrySectionTop = [];
		const arrySectionHeight = [];
		const arrySectionSum = [];
		
		for(var i=0; i<$fixedBox.length; i++) {
			arrySectionTop.push($fixedBox.eq(i).offset().top);
			arrySectionHeight.push($fixedBox.eq(i).outerHeight());
			arrySectionSum.push(arrySectionTop[i] + arrySectionHeight[i] - offset);

			if ($(window).scrollTop() > arrySectionTop[i] - offset  && $(window).scrollTop() <= arrySectionSum[i] - checkTiming) {
				$fixedBox.eq(i).find('.tit-sec').addClass("fixed");
				if ($(window).scrollTop() > arrySectionSum[i] - checkTiming) { 
					$fixedBox.eq(i).find('.tit-sec').addClass("in");
				} else {
					$fixedBox.eq(i).find('.tit-sec').removeClass("in");
				}
			} else {
				$fixedBox.eq(i).find('.tit-sec').removeClass("fixed");
			}
		}
	}

	$(window).on('load scroll resize', function(){
		setMenufix2();
		setAniFix();
		//setTitlefix();
	});

	$(window).on('scroll', function(){
		setSection();
		setSection2();
		handleProgress();

		$('.motion2').each(function(i){
			if(checkVisible($(this))){
				$(this).addClass('in');
			}
		});
	});

	// 엘리먼트 체크
	function checkVisible( elm, eval ) {
		eval = eval || "object visible";
		var viewportHeight = $(window).height(), // Viewport Height
			scrolltop = $(window).scrollTop(), // Scroll Top
			y = $(elm).offset().top,
			elementHeight = $(elm).height();   
		
		if (eval == "object visible") return ((y < (viewportHeight + scrolltop)) && (y > (scrolltop - elementHeight)));
		if (eval == "above") return ((y < (viewportHeight + scrolltop)));
	}

	// styling swiper
	function stylingSwiper() {
		var stylingSwiper = new Swiper('.styling-swiper', {
			spaceBetween: 0,
			slidesPerView: 'auto',
			simulateTouch: true,
			loop: false,
		});
	}
	stylingSwiper();
	
	// 영상 컨트롤
	var videoEvent = document.querySelector("#event-section-01 .vod"); 
	$(".section-01 .movi-sec").on("click", ".btn-movie", function(){
		videoEvent.play();
		$(this).parent().addClass("off");
	});
	$(".section-01 .movi-sec").on("click", function(){
		$(this).children(".pmo-btn-top").addClass("off");
	});

	// 프로그레스바
	var handleProgress = function(){
		var lastTop = $('.event-menu').position().top;
		var lastH = $('.event-menu').outerHeight()-offset;
		var posSum = lastTop + lastH;
		var progressIndicator = $(".progress-bar");
		
		var currentPercentage = ($(window).scrollTop() / ($(document).outerHeight() - $(window).height())) * 100;
		progressIndicator.css("width", currentPercentage+'%');
	};

	// 유의사항 토글 
	$('.list-notice dt').click(function(){
		$(this).parent().toggleClass('on');
	});

	// 상단 메뉴 토글
	$(".top-menu li").hover(function() {
		$(this).children(".box-hash").addClass("on");
	}, function(){
		$(this).children(".box-hash").removeClass("on");
	});

	// 가이드 화살표 토글
	$(".guide-banner .list-banner li .img-sec").hover(function() {
		$(this).children(".arrow").addClass("on");
	}, function(){
		$(this).children(".arrow").removeClass("on");
	});

    // 댓글 영역 이동
    $(".ins-event .btn-reply").on("click", function(e){
		e.preventDefault;
        var offsetTop = $(".detail-footer-wrap").offset().top;
        $("html").stop().animate({
            scrollTop: offsetTop - 40,
        }, 0);
    });
	
	// 화보 1번 영역 fixed
	// var setMenufix = function(){
	// 	var lastTop = $('.insider-item-box:last-of-type').offset().top;
	// 	var lastH = $('.insider-item-box:last-of-type').outerHeight()-offset;
	// 	var posSum = lastTop + lastH;
		
	// 	if($(window).scrollTop() > posSum - 700){
	// 		$('.insider-item-box:last-of-type').addClass('fix-last');
	// 	} else {
	// 		$('.insider-item-box:last-of-type').removeClass('fix-last');
	// 	}
	// };

	// 화보 1번 영역 fixed
	var setSection2 = function(){
		var $sections = $context.find('.insider-item-box');
		$sections.each(function(idx, $section){
			if ($(window).scrollTop() > $($section).offset().top - offset - 30 && $(window).scrollTop() <= $($section).offset().top + $($section).height()) {
				$sections.removeClass('active');
				$sections.eq(idx).addClass('active');
			} else {
				$sections.eq(idx).removeClass('active');
			}
		});
	};
	setSection2();

	// 카탈로그 레이어 팝업 열기
	$(".catalogue-box .list-top-thumb li > a").on("click", function(){
		$(this).next(".layer-top").addClass("on");
		$(".zoomContainer").css("display", "none"); // 줌 예외처리
	});	

	// 카탈로그 레이어 팝업 닫기
	$(".catalogue-box .btn-close-layer").on("click", function(){
		$(this).closest(".layer-top").removeClass("on");
		$(".zoomContainer").css("display", "block"); // 줌 예외처리
	});
	$(".catalogue-box .dim").on("click", function(){
		console.log("on");
		$(this).closest(".layer-top").removeClass("on");
		$(".zoomContainer").css("display", "block"); // 줌 예외처리
	});

	// 카탈로그 컬러 픽업
	$(".zoom-sec .list-color li").on("click", function(idx){
		console.log(idx)
		// $(".zoom-sec .list-prd li").removeClass("on");
		// $(this).parent().siblings(".list-prd") li:nth-child(" + (idx + 1) +")").addClass("on");
	});

