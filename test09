	function handleScrollMagic() {
		var controller = new ScrollMagic.Controller();

		// var sceneArray = new Array()

		/**************************************** Scene0 start ****************************************/
			// Scene0 : 상단 인트로 이미지 고정
			var Scene0 = new TimelineMax();

			//let height = $(".trigger1 .intro").height();
			var Scene0 = new ScrollMagic.Scene({
				triggerElement: ".trigger0",
				triggerHook: "onLeave",
				duration: "100%",

			})
			.setPin(".trigger0")
			//.addIndicators({name : "scene0"})
			.addTo(controller);

			//Scene1.offset(-20);

			// Scene0_2 : 상단 인트로 이미지 모션, 중앙 텍스트
			var tlScene0_2 = new TimelineMax();

			tlScene0_2
			.staggerFromTo(".trigger0 .list-txt li img", 2, {opacity: 0, y: "40px"}, {opacity: 1, y: "0px"}, 0.4)

			var Scene0_2 = new ScrollMagic.Scene({
				triggerElement: ".trigger0",
				triggerHook: 0,
				//duration: "150%",

			})
			.setTween(tlScene0_2)
			//.addIndicators({name : "Scene0_2"})
			.addTo(controller);


			// // Scene0_2 : 상단 인트로 이미지 모션, 중앙 텍스트
			// var tlScene0_2 = new TimelineMax();

			// tlScene0_2
			// .set(".trigger0 .top-box .bg1", {autoAlpha: 0.5})
			// .to(".trigger0 .top-box .bg1", 3, {autoAlpha: 0})
			// .from(".trigger0 .top-box .bg2", 3, {top: "100%"}, "-=3")
			// .to(".trigger0 .top-box .bg2", 3, {top: "0%"}, "-=3")

			// var Scene0_2 = new ScrollMagic.Scene({
			// 	triggerElement: ".trigger0",
			// 	triggerHook: 0,
			// 	duration: "150%",

			// })
			// .setTween(tlScene0_2)
			// //.addIndicators({name : "Scene0_2"})
			// .addTo(controller);
		/**************************************** /Scene0 end ****************************************/

		// removeIndicators
		Scene0.removeIndicators();
		Scene0_2.removeIndicators();

	}

	handleScrollMagic();	

})($('.pmo-cont'), $('html,body'));

// URL 키값을 위한 정의
function getParameterByName(name) {
	name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	const regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),results = regex.exec(location.search);
	return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

function chkHashUrl() {
	const chkParamUrl2 = getParameterByName('event02');	// MODERN NOMADS
	const chkParamUrl3 = getParameterByName('event03'); // STYLING
	const chkParamUrl4 = getParameterByName('event04'); // EVENT
	const eventTop02 = $('#event-section-02').offset().top;
	const eventTop03 = $('#event-section-03').offset().top;
	const eventTop04 = $('#event-section-04').offset().top;
	const offset = 84;

	if(chkParamUrl2) {
		$("html").stop().animate({
			scrollTop: eventTop02 - offset,
		}, 400);
	} else if(chkParamUrl3) {
		$("html").stop().animate({
			scrollTop: eventTop03 - offset,
		}, 400);
	} else if(chkParamUrl4) {
		$("html").stop().animate({ 
			scrollTop: eventTop04 - offset,
		}, 400);
	} else {
		// $(window).scrollTop(0); 
	}
}

setTimeout(function() {
    chkHashUrl();
}, 400);					
</script>

