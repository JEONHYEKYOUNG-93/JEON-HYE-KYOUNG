								<script>
								(function($context, $doc){
									if (!$context.length) return;
									
									// 스크롤 메뉴
									var $menu = $context.find('.event-menu');
									var $menuanchor = $context.find('.menu-anchor');
									var $menus = $context.find('.event-menu li');
									var $menubox = $menu.find('.event-menu-inner');
									var $sections = $context.find('section');
									var offset = 84;
									var setMenufix = function(){
										var lastTop = $('#event-section-04').position().top
										var lastH = $('#event-section-04 ').outerHeight()-offset;
										var posSum = lastTop + lastH
										if ($(window).scrollTop() > $menu.offset().top - offset  && $(window).scrollTop() <= posSum) {
											$menubox.addClass('fixed');
										}
										else {
											$menubox.removeClass('fixed');
										}
									};
									var setSection = function(){
										$sections.each(function(idx, $section){
											if ($(window).scrollTop() >= $section.offsetTop - offset) {
												$menus.removeClass('active');
												$menus.eq(idx).addClass('active');
											}
										});
									};

									// setMenufix();
									// setSection();
									
									$menu.on('click', 'a', function (e) {

										var section = $(e.currentTarget.hash);

										if (!section.length) return;
										try {
											var offsetTop = Math.round(section.offset().top);
									
											$doc.stop().animate({
												scrollTop: offsetTop - offset + 2
											}, 400);
									
											e.preventDefault();
										}
										catch(e) {
											console && console.log(e)
										}
									});

									$menuanchor.on('click', function (e) {

										var section = $(e.currentTarget.hash);

										if (!section.length) return;
										try {
											var offsetTop = Math.round(section.offset().top - 100);

											$doc.stop().animate({
												scrollTop: offsetTop - offset + 2
											}, 400);

											e.preventDefault();
										}
										catch(e) {
											console && console.log(e)
										}
									});

									$(window).scrollTop(1);
									$(window).on('scroll', function(){
										// setMenufix();
										// setSection();

										$('.motion').each(function(i){
											// var sum = delay*i
											if(checkVisible($(this))){
												$(this).addClass('in');
											}
											
										});
										
									});

									$('.top-box .motion').each(function(i){
										if(checkVisible($(this))){
											$(this).addClass('in');
										}
									});

									function checkVisible( elm, eval ) {
										eval = eval || "object visible";
										var viewportHeight = $(window).height(), // Viewport Height
											scrolltop = $(window).scrollTop(), // Scroll Top
											y = $(elm).offset().top,
											elementHeight = $(elm).height();   
										
										if (eval == "object visible") return ((y < (viewportHeight + scrolltop)) && (y > (scrolltop - elementHeight)));
										if (eval == "above") return ((y < (viewportHeight + scrolltop)));
									}
									
									function popSwiper(){
										$(".look-slider-box").each(function(index){
											var $this = $(this);
											var lookSwiper = new Swiper('.instance.swiper' + index, {
												init: false,
												loop: true,
												spaceBetween: 0,
												speed: 600,
												slidesPerView: 'auto',
												allowTouchMove: true,
												autoplay: {
												delay: 3000,
												disableOnInteraction: false,
												},
												navigation: {
													prevEl: $this.find(".swiper-button-prev")[0],
													nextEl: $this.find(".swiper-button-next")[0],
												},
											});
											lookSwiper.init();
										});
									}
									popSwiper();

									// 브랜드리스트 랜덤 적용
									randomList();
									function randomList(){
										$(".js-randomList").each(function(index){
											// bo 등록시 랜덤 적용
											var len = $(this).find('.item').length;
											var ul = $(this);
											var liArr = ul.children('.item');
											liArr.sort(function() {
												var temp = parseInt(Math.random() * len);
												var temp1 = parseInt(Math.random() * len);
												return temp1 - temp;
											}).appendTo(ul);

										});
									}

									//유의사항 토글 
                                    $('.list-notice dt').click(function(e){
                                        $(this).closest('dl').toggleClass('on')
                                        e.preventDefault();
                                    });

									function fnOuterSwiper() {
                                        var thumbnailSwipers = [];
                                        var bigBannerSwipers = [];

                                        $('.thumbnail-swiper').each(function(idx, el) {
                                            var thumbnailSwiper = new Swiper($(this).addClass(`thumbnail-banner-${idx}`), {
                                                init: false,
                                                spaceBetween: 15,
                                                slidesPerView: 10,
                                                freeMode: true,
                                                touchRatio: 1,
                                                autoplay: false,
                                                watchSlidesProgress: true,
                                            });
                                            thumbnailSwipers.push(thumbnailSwiper);
                                            thumbnailSwipers[idx].init();
                                        });

                                        $('.big-banner-swiper').each(function(idx, el) {
                                            var bigBannerSwiper = new Swiper($(this).addClass(`big-banner-${idx}`), {
                                                init: false,
                                                effect : 'fade',
                                                    fadeEffect: {
                                                        crossFade: true
                                                    },
                                                spaceBetween: 0,
                                                slidesPerView: 1,
                                                touchRatio: 0,
                                                autoplay:false,
                                                thumbs: {
                                                    swiper: thumbnailSwipers[idx],
                                                },
                                            });
                                            bigBannerSwipers.push(bigBannerSwiper);
                                            bigBannerSwipers[idx].init();
                                        });
                                    }
                                    fnOuterSwiper();

									// 탭
									$("ul.tabs li").on("click", function (evt) {
                                        evt.preventDefault();
                                        $(this).addClass("on");
                                        $(this).siblings().removeClass("on");

                                        var activeTab = $(this).attr("data-title");
                                        $(".tab-" + activeTab).addClass("on").siblings().removeClass('on');
                                        $(".brand-list .item").removeClass("on");  // 탭 이동시 배너 히든 초기화
                                        $(".bt-more").addClass("on");  // 탭 이동시 버튼 히든 초기화
                                    });

								})($('.pmo-cont'), $('html,body'));

								// 아이템 더보기
								function handleMore(items, target) {    // target 으로 div 클래스를 전달 받아 함수를 재활용 할 수 있도록 설정
									items.preventDefault;

									$(target).find(".item:hidden").slice(0, items).addClass("on"); // 숨김 설정된 다음 n개를 선택하여 표시
									
									console.log($(target).find(".item:hidden").length);
									if ($(target).find(".item:hidden").length === 0){ // 숨겨진 아이템이 있는지 체크
										$(target).find(".bt-more").removeClass("on");
									}
								}

								</script>
								<!-- //E : 이벤트 상세 코너 -->

							<!--E : 이벤트 컨텐츠 임시 영역-->
						</div>
						<!--E : 이벤트 컨텐츠 html 영역-->
					</div>

