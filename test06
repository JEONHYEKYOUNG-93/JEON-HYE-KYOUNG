					.rouletteEvent_2106 { position: relative; top: 0; left: 50%; -webkit-transform: translateX(-50%); transform: translateX(-50%); width: 1920px;height:760px;}
					.rouletteEvent_2106 img { vertical-align: middle; }
					.rouletteEvent_2106 .rouletteBoard { position: absolute; top: 20px; left: 584px; z-index: 2; }
					.rouletteEvent_2106 .rouletteBoard .rleb_bg { position: relative; z-index: 99; }
					.rouletteEvent_2106 .rouletteBoard .rleb_rotate { position: absolute; top: 10px; left: 10px; z-index: 100; }
