					.rouletteEvent_2106 .rouletteBoard .rleb_needle { position: absolute; top: -17px; left: 349px; z-index: 103; }
					.rouletteEvent_2106 .rouletteBoard .rleb_btnStart { position: absolute; top: 257px; left: 257px; -webkit-animation-name: all; animation-name: all; -webkit-animation-delay: 0s; animation-delay: 0s; -webkit-animation-duration: 0.3s; animation-duration: 0.3s; -webkit-animation-timing-function: ease-in-out; animation-timing-function: ease-in-out; z-index: 200; }
