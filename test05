<script>
	(function($context, $doc){
		if (!$context.length) return;
		var $menu = $context.find('.event-menu');
		var $menus = $context.find('.event-menu li');
		var $menubox = $menu.find('.event-menu-inner');
		var $sections = $context.find('section');
		var offset = 84;

		// 프로필
		$.fn.Scrolling = function(obj, tar) {
			var _this = this;
			$(window).scroll(function(e) {
				var end = obj + tar;
				$(window).scrollTop() >= obj ? _this.addClass("fixed") : _this.removeClass("fixed");
				if($(window).scrollTop() >= end) _this.removeClass("fixed");
			});
		};

		$(window).on('load scroll', function(){
			setMenufix();
			setSection();
			$(".btn-follow").Scrolling($(".items-wintershoes").offset().top - 150, ($(".items-wintershoes").height() - $(".btn-winter-submit").height()));
			$('.motion').each(function(i){
				if(checkVisible($(this))){
					$(this).addClass('in');
				}
			});
		});


		// 탭 고정
		var setMenufix = function(){
			var lastTop = $('#section03').position().top
			var lastH = $('#section03').outerHeight()-offset;
			var posSum = lastTop + lastH
			if ($(window).scrollTop() > $menu.offset().top - offset  && $(window).scrollTop() <= posSum) {
				$menubox.addClass('fixed');
			}
			else {
				$menubox.removeClass('fixed');
			}
		};

		// 탭 헤드 on, off
		var setSection = function(){
			$sections.each(function(idx, $section){
				if ($(window).scrollTop() >= $section.offsetTop - offset - 40) {
					$menus.removeClass('active');
					$menus.eq(idx).addClass('active');
				}
			});
		};

		// 탭 클릭 이동
		var handleClickTab = function(){
			$menus.each(function(idx, el){
				$menus.eq(idx).on("click", "a", function(e) {
					e.preventDefault();
					var section = $(e.currentTarget.hash);
					var offsetTop = Math.round(section.offset().top);
					idx === 0 ? $doc.stop().animate({scrollTop: offsetTop - offset}, 400) : $doc.stop().animate({scrollTop: offsetTop - offset - 40}, 400);		// 첫번째 탭 예외 처리 idx === 0
				});	
			});
		};
		handleClickTab();

		function popSwiper(){
			$(".gift-swiper").each(function(index, element){
				var $this = $(this);
				$this.addClass("type-div-" + index);
				var giftSwiper = new Swiper(".type-div-" + index, {
					init: false,
					effect: 'slide',
					spaceBetween: 130,
					speed: 400,
					// autoplay:  {
					//     delay: 1,
					//     disableOnInteraction : false,
					// },
					loop: false,
					slidesPerView: "auto",
					allowTouchMove: true,
					/*navigation: {
						prevEl: '.btn-swiper-arrow.prev',
						nextEl: '.btn-swiper-arrow.next',
					}*/
				});
				giftSwiper.init();
			});
		}
		popSwiper();

		// 엘리먼트 view 체크
		function checkVisible( elm, eval ) {
			eval = eval || "object visible";
			var viewportHeight = $(window).height(), // Viewport Height
				scrolltop = $(window).scrollTop(), // Scroll Top
				y = $(elm).offset().top,
				elementHeight = $(elm).height();

			if (eval == "object visible") return ((y < (viewportHeight + scrolltop)) && (y > (scrolltop - elementHeight)));
			if (eval == "above") return ((y < (viewportHeight + scrolltop)));
		};

		$('.evt-top .motion').each(function(i){
			if(checkVisible($(this))){
				$(this).addClass('in');
			}
		});

		// 유의사항 토글 
		$('.list-notice dt').click(function(){
			$(this).parent().toggleClass('on');
		});	

})($('.pmo-cont'), $('html,body'));
</script>

