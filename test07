<script>
(function($context, $doc){
	if (!$context.length) return;
	
	// 스크롤 메뉴
	var $menu = $context.find('.event-menu');
	var $menus = $context.find('.event-menu li');
	var $menubox = $menu.find('.event-menu-inner');
	var $sections = $context.find('section');
	var offset = 84;
	var tabHeight = 40;

	var setMenufix = function(){
		var lastTop = $('#event-section-04').position().top
		var lastH = $('#event-section-04').outerHeight()-offset;
		var posSum = lastTop + lastH
		if ($(window).scrollTop() > $menu.offset().top - offset  && $(window).scrollTop() <= posSum) {
			$menubox.addClass('fixed');
		}
		else {
			$menubox.removeClass('fixed');
		}
	};

	var setSection = function(){
		$sections.each(function(idx, $section){
			if ($(window).scrollTop() >= $section.offsetTop - offset - tabHeight - 1) {
				$menus.removeClass('active');
				$menus.eq(idx).addClass('active');
			}
		});
	};

	// 섹션 타이틀 고정
	function setTitlefix() {
		const checkTiming = 400;
		const arrySectionTop = [];
		const arrySectionHeight = [];
		const arrySectionSum = [];
		
		for(var i=0; i<$sections.length; i++) {
			arrySectionTop.push($sections.eq(i).offset().top);
			arrySectionHeight.push($sections.eq(i).outerHeight());
			arrySectionSum.push(arrySectionTop[i] + arrySectionHeight[i] - offset);

			if ($(window).scrollTop() > arrySectionTop[i] - offset  && $(window).scrollTop() <= arrySectionSum[i] - checkTiming) {
				$sections.eq(i).find('.tit-sec').addClass("fixed");
				if ($(window).scrollTop() > arrySectionSum[i] - checkTiming) { 
					$sections.eq(i).find('.tit-sec').addClass("in");
				} else {
					$sections.eq(i).find('.tit-sec').removeClass("in");
				}
			} else {
				$sections.eq(i).find('.tit-sec').removeClass("fixed");
			}
		}
	}

	// 탭 클릭 이동
	$menu.on('click', 'li a', function (e) {
		var section = $(e.currentTarget.hash);
		var offsetTop = Math.round(section.offset().top);
		$doc.stop().animate({
			scrollTop: offsetTop - offset - tabHeight,
		}, 400);
		e.preventDefault();
	});

	// 첫번째 탭 예외 처리
	$menu.on('click', 'li:nth-child(1) a', function (e) {
		var section = $(e.currentTarget.hash);
		var offsetTop = Math.round(section.offset().top);
		$doc.stop().animate({
			scrollTop: offsetTop - offset,
		}, 400);
		e.preventDefault();
	});

	$(window).on('load scroll resize', function(){
		setMenufix();
		setTitlefix();
	});

	$(window).on('scroll', function(){
		setSection();

		$('.motion').each(function(i){
			if(checkVisible($(this))){
				$(this).addClass('in');
			}
		});	
	});

	// 카운트 다운
	const targetDate = "12 June 2022 23:59:59 GMT+0900";
	function countdown() {
		const newTargetDate = new Date(targetDate);
		const currentDate = new Date();
		if(currentDate < newTargetDate) {    // 1차 오픈
			const totalSeconds = (newTargetDate - currentDate) / 1000;
			const days = Math.floor(totalSeconds / 3600 / 24);
			const hours = Math.floor(totalSeconds / 3600) % 24;
			const minutes = Math.floor(totalSeconds / 60) % 60;
			const seconds = Math.floor(totalSeconds) % 60;
			document.getElementById('days').innerText = formatTime(days);
			document.getElementById('hours').innerText = formatTime(hours);
			document.getElementById('minutes').innerText = formatTime(minutes);
			document.getElementById('seconds').innerText = formatTime(seconds);
		} else {    // 이벤트 종료
			return;
		}
	}
	function formatTime(time) {
		return time < 10 ? "0" + time : time;
	}
	setInterval(countdown, 1000);	


	/**************** 스크롤 매직 애니메이션 시작 ***************/
	// 스크롤 매직
	var controller = new ScrollMagic.Controller();

	// 탑 비주얼 컨테이너
	var scene0 = new ScrollMagic.Scene({
		duration: "1500",
		offset: 280,
	})
	.setPin('#animateTop0')
	.addTo(controller)
	// .addIndicators({name: "setSpin"});	

	// animation 탑 비주얼 타이틀
	var tween1 = TweenMax.to('#animateTop1', 0.5, {
		opacity: 1,
		x: 0,
		y: 0,
	});
	var scene1 = new ScrollMagic.Scene({
		triggerElement: "animateTop1",
		duration: "800",
		offset: 280,
	})
	.setTween(tween1)
	.addTo(controller)
	// .addIndicators({
	// 	name: "1"
	// });
	
	// animation 핸드폰
	var tween2 = TweenMax.to('#animateTop2', 0.5, {
		scale: 1,
		x: 0,
		y: -10,
	});
	var scene2 = new ScrollMagic.Scene({
		duration: "800",
		offset: 280,
	})
	.setTween(tween2)
	.addTo(controller)
	// .addIndicators({
	// 	name: "2"
	// });	

	// animation 비주얼 서브 텍스트
	var tween3 = TweenMax.to('#animateTop3', 0.5, {
		opacity: 1,
		x: 0,
		y: 0,
	});
	var scene3 = new ScrollMagic.Scene({
		duration: "1000",
		offset: 800,
	})
	.setTween(tween3)
	.addTo(controller)
	// .addIndicators({
	// 	name: "3"
	// });	
	/**************** 스크롤 매직 애니메이션 끝 ***************/

	// 당첨 확률 높이기 버튼
	$(".time-flex .pmo-btn a").on("click", function(){
		console.log("클릭")
		var scrollPosition = $("#comment").offset().top;
		$("html").animate({
			scrollTop: scrollPosition - offset - tabHeight,
		}, 500);			
	});

	// 유의사항 토글 
	$('.list-notice dt').click(function(){
		$(this).parent().toggleClass('on');
	});	

	// 컨텐츠 스크롤바
	// detailScrollBars = OverlayScrollbars($('.killing-time .banner-sec'), {
	// 	className : "os-theme-light",
	// });

	// 엘리먼트 체크
	function checkVisible( elm, eval ) {
		eval = eval || "object visible";
		var viewportHeight = $(window).height(), // Viewport Height
			scrolltop = $(window).scrollTop(), // Scroll Top
			y = $(elm).offset().top,
			elementHeight = $(elm).height();   
		
		if (eval == "object visible") return ((y < (viewportHeight + scrolltop)) && (y > (scrolltop - elementHeight)));
		if (eval == "above") return ((y < (viewportHeight + scrolltop)));
	}	

	/* 220527 수정 */
	// 주소 복사
	var copyBtn = document.querySelector(".pmo-btn-share button");
	copyBtn.addEventListener("click", function () {
		var copy_url = document.location.href;
		console.log(copy_url);
		navigator.clipboard.writeText(copy_url);
	}, false);
	/* /220527 수정 */

})($('.pmo-cont'), $('html,body'));

// URL 키값을 위한 정의
function getParameterByName(name) {
	name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	const regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),results = regex.exec(location.search);
	return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

function chkHashUrl() {
	const chkParamUrl2 = getParameterByName('event02');	// 타임 쿠폰
	const chkParamUrl3 = getParameterByName('event03'); // 타임 어택
	const chkParamUrl4 = getParameterByName('event04'); // 킬링 타임 /* 220530 수정2 */
	const eventTop02 = $('#event-section-02').offset().top;
	const eventTop03 = $('#event-section-03').offset().top;
	const eventTop04 = $('#event-section-04').offset().top; /* 220530 수정2 */
	const offset = 84;

	if(chkParamUrl2) {
		$("html").stop().animate({
			scrollTop: eventTop02 - offset,
		}, 400);
	} else if(chkParamUrl3) {
		$("html").stop().animate({
			scrollTop: eventTop03 - offset,
		}, 400);
	} else if(chkParamUrl4) { // 킬링 타임 /* 220530 수정2 */
		$("html").stop().animate({ 
			scrollTop: eventTop04 - offset,
		}, 400);
	} else {
		// $(window).scrollTop(0); 
	}
}

setTimeout(function() {
    chkHashUrl();
}, 400);
</script>
